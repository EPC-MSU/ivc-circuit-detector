{
  "name": "Dataset generation parameters settings file",
  "description": "This file used for variate element parameters in .cir files. For example, resistance of R1 in some schema will be variate in interval from 1 to 100000000 with 10 point in exponential scale. This action produces 10 new .cir files with different resistance.",
  "version": "0.0.2",
  "elements" : {
    "R": [
      {
        "name": "resistance",
        "units": "kOhm",
        "cir_key": null,
        "cir_unit": "K",
        "nominal" : {
          "type": "exponential_interval",
          "interval": [1, 100000000],
          "interval_points": 10
        }
      }
    ],
    "C" : [
      {
        "name": "capacity",
        "units": "nF",
        "cir_key": null,
        "cir_unit": "N",
        "nominal" : {
          "type": "uniform_interval",
          "interval": [1, 1000],
          "interval_points": 10
        }
      }
    ],
    "L": [
      {
        "name": "inductance",
        "units": "mH",
        "cir_key": null,
        "cir_unit": "M",
        "nominal" : {
          "type": "uniform_interval",
          "interval": [1, 10],
          "interval_points": 10
        }
      }
    ],
    "D": [
      {
        "name": "saturation current",
        "units": "A",
        "cir_key": "Is",
        "cir_unit": null,
        "nominal" : {
          "type": "constant",
          "value": 1e-15
        }
      },
      {
        "name": "junction potential",
        "units": "V",
        "cir_key": "Vj",
        "cir_unit": null,
        "nominal" : {
          "type": "constant",
          "value": 0.7
        }
      },
      {
        "name": "reverse breakdown voltage",
        "units": "V",
        "cir_key": "Bv",
        "cir_unit": null,
        "nominal" : {
          "type": "constant",
          "value": 10
        }
      },
      {
        "name": "current at reverse breakdown voltage",
        "units": "V",
        "cir_key": "Ibv",
        "cir_unit": null,
        "nominal" : {
          "type": "constant",
          "value": 0.7
        }
      }
    ]
  }
}